{
  "name": "orkut",
  "version": "1.0.0",
  "description": "",
  "main": "src/server.js",
  "scripts": {
    "start": "ts-node src/server.ts",
    "build": "tsc",
    "start-prod": "tsc && node build/server.js",
    "predev": "npm run build && npm run db:reset",
    "dev": "nodemon --watch \"./src/**\" --ext \"ts,js\" --ignore \"src/**/*.spec.ts,src/**/*.json\" --exec \"ts-node src/server.ts\"",
    "db:reset": "npx -y tsc && npx sequelize-cli db:drop && npx sequelize-cli db:create && npx sequelize-cli db:migrate && npx sequelize db:seed:all"
  },
  "keywords": [],
  "author": "",
