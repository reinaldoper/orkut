    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.4",
    "@types/multer": "^1.4.11",
    "@types/node": "^16.11.16",
    "@types/sequelize": "^4.28.14",
    "mysql2": "^2.3.3",
    "nodemon": "^2.0.20",
    "sequelize-cli": "^6.5.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@types/amqplib": "^0.10.5",
    "@types/bcrypt": "^5.0.0",
